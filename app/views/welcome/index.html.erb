<% provide :page_title, 'Welcome' %>
<% currently_at 'Welcome' %>
<div class="text-center mt-5">
  <h1 class="display-1  mb-5">Welcome to <span class="text-primary">Wallet App</span></h1>
  <h2 >
    Please, <%= link_to "Log In", login_path, class: 'btn btn-primary btn-lg', :method => :get %>
    or <%= link_to "Sign Up", signup_path, class: 'btn btn-primary btn-lg', :method => :get %>
    to proceed.
  </h2>
</div>